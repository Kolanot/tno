                owl:allValuesFrom :Amount ;
                owl:onProperty :measures
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty :measures ;
                owl:someValuesFrom :Amount
              ] .

