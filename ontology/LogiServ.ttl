      owl:disjointWith :Consolidation , :Deconsolidation , :Discharge , :Load , :LoadTransportDischarge , :Storage , :Transport , :Transshipment .

:Netherlands
      rdf:type logico:Location ;
      rdfs:label "Netherlands"^^xsd:string .

