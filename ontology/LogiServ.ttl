              [ rdf:type owl:Restriction ;
                owl:onProperty :governmentActionCode ;
                owl:someValuesFrom xsd:int
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty :governmentInvolvmentCode ;
                owl:someValuesFrom xsd:int
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
